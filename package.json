{
  "name": "frontendtemplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.jsx": "eslint --fix",
    "*.ts": "eslint --fix",
    "*.tsx": "eslint --fix",
    "*.{css,scss,json,md,html,xml,yml}": "prettier --write",
    "*.{png,jpg,jpeg,gif,svg,woff,woff2,eot,ttf,otf}": "echo \"Asset files are not linted.\""
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.+(ts|tsx|json|scss|md)\"",
    "precommit": "lint-staged",
    "analyze": "webpack-bundle-analyzer",
    "deploy": "npm run build && node deploy.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BhupeshChauhan/frontendtemplate.git"
  },
