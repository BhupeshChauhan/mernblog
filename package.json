{
  "name": "frontendtemplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.jsx": "eslint --fix",
    "*.ts": "eslint --fix",
    "*.tsx": "eslint --fix",
    "*.{css,scss,json,md,html,xml,yml}": "prettier --write",
    "*.{png,jpg,jpeg,gif,svg,woff,woff2,eot,ttf,otf}": "echo \"Asset files are not linted.\""
  },
  "scripts": {
    "start": "webpack serve --config webpack/webpack.config.js --env env=dev",
    "build": "webpack --config webpack/webpack.config.js --env env=prod",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.+(ts|tsx|json|scss|md)\"",
    "precommit": "lint-staged",
    "analyze": "webpack-bundle-analyzer",
    "deploy": "npm run build && node deploy.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BhupeshChauhan/frontendtemplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BhupeshChauhan/frontendtemplate/issues"
  },
  "homepage": "https://github.com/BhupeshChauhan/frontendtemplate#readme",
  "dependencies": {
    "lucide-react": "^0.274.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/eslint-parser": "^7.22.11",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.11",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@types/node": "^20.5.4",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.15",
    "babel-loader": "^9.1.3",
    "compression-webpack-plugin": "^10.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "less-loader": "^11.1.3",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.29",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.1.2",
    "prettier": "^3.0.2",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
